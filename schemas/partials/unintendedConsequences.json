                    "type": "string",
                    "title": "Description"
                },
                "outcome": {
                    "title": "Outcome",
                    "type": "string",
                    "enum": ["Positive", "Negative"]
                },
                "impact": {
                    "type": "string",
                    "enum": ["High", "Medium", "Low"]
                },
                "likelihood": {
