{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "unintendedConsequences": {
            "type": "array",
            "title": "Unintended Consequences",
            "items": {
                "type": "object",
                "title": "Unintended Consequence",
                "properties": {
                    "consequence": {
                        "type": "string",
                        "title": "Description"
                    },
                    "outcome": {
                        "title": "Outcome",
                        "type": "string",
                        "enum": ["Positive", "Negative"]
                    }
                },
                "required": ["consequence"]
            }
        }
    },
    "required": ["unintendedConsequences"]
}